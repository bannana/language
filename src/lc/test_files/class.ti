class Counter:
{
	int data = 0;

	func count -> int:
	{
		data = data + 1;

		return data;
	}

	func reset -> void:
	{
		data = 0;
	}
}

class ObjectFibb:
{
	int a = 0;
	int b = 1;
	int c = 0;

	Counter acc = new Counter();

	func next -> void:
	{
		if c == 0:
		{
			b = a + b;
			c = 1;
		} else
		if c == 1:
		{
			a = a + b;
			c = 0;
		}
	}
	
	func display -> void:
	{
		print b;
		print a;
	}

	func run (int count) -> void:
	{
		acc.reset();
		while acc.count() < count:
		{
			next();
			display();
		}
	}
}

ObjectFibb test = new ObjectFibb();

print test.c.data;

test.run(15);
