class Counter:
{
	int data = 0;

	func count -> int:
	{
		data = data + 1;

		return data;
	}

	func reset -> void:
	{
		data = 0;
	}
}

class ObjectFibb:
{
	int a = 0;
	int b = 1;

	Counter acc = new Counter();

	func next -> void:
	{
		b = a + b;
		a = a + b;
	}
	
	func display -> void:
	{
		print b;
		print a;
	}

	func run (int count) -> void:
	{
		acc.reset();
		while acc.count() < count:
		{
			next();
			display();
		}
	}
}

ObjectFibb test = new ObjectFibb();

test.run(15);
print "ALL DONE!";
print test.a;
print test.b;
print test.acc.data;

