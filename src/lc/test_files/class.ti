class Counter:
{
	int val = 0;
	int inc = 1;

	func increment -> int:
	{
		val = val + inc;
		return val;
	}
}

class ObjectFibb:
{
	int a = 0;
	int b = 1;
	int c = 0;

	Counter counter = new Counter();

	func next -> void:
	{
		if c == 0:
		{
			b = a + b;
			c = 1;
		} else
		if c == 1:
		{
			a = a + b;
			c = 0;
		}
	}
	
	func display -> void:
	{
		print b;
		print a;
	}

	func run (int count) -> void:
	{
		counter.val = 0;
		while counter.increment() < count:
		{
			next();
			display();
		}
	}
}

ObjectFibb test = new ObjectFibb();

test.run(15);
